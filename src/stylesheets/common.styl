.cm-fl{
  float: left;
}
.cm-fr{
  float: right;
}
.cm-cb::after{
  content: '';
  height: 0;
  overflow: hidden;
  clear: both;
}
.cm-flex-col{
  display: flex;
  align-items: center;
  justify-content: center;
  /*flex-flow: column nowrap;*/
}
.cm-flex1{
  flex: 1
}
.cm-w100{
  width: 100%;
}
.cm-ab{
  position: absolute;
}
.cm-fix{
  position: fixed;
}
.cm-rel{
  position: relative;
}
.cm-left-btn{
  width: 12px;
  height: 12px;
  border-top: 1px solid #fff;
  border-left: 1px solid #fff;
  transform: rotateZ(-45deg);
  /*position: absolute;*/
}
.cm-right-btn{
  width: 12px;
  height: 12px;
  display: inline-block;
  border-top: 1px solid #fff;
  border-right: 1px solid #fff;
  transform: rotateZ(45deg);
  /*top: 9px;*/
  /*position: absolute;*/
  /*right: 14px;*/
}
.cm-main{
  position: fixed;
  top: 40px;
  bottom: 0;
  width: 100%;
  z-index: 20;
}
.cm-btn{
    height: 40px;
    border-radius: 4px;
    line-height: 40px;
    text-align: center;
}
.cm-middle{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate3d(-50%, -50%, 0);
}
.cm-elis{
    overflow:hidden
    white-space: nowrap;
    text-overflow: ellipsis
}
.cm-title{
    font-size $font-14
    padding 6px 0
}
